% Fill reference channel for NPtype21 probes
% shank, channel and geometry should be set in the prm file
% These strings can be generated by SGLXMetaToCoords
channels = 1:4;

% Site location in micrometers (x and y)
geometry = [0 32 0 32; 0 0 15 15]';

% Reference sites ch being excluded
ref_chan = 127;

% Recording contact pad size in micrometers. Height x width
pad = [12 12];